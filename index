<script>
// Matrix rain effect
const canvas = document.createElement('canvas');
const ctx = canvas.getContext('2d');
document.querySelector('.matrix-bg').appendChild(canvas);

canvas.width = window.innerWidth;
canvas.height = window.innerHeight;

const chars = 'FWOGAI$0123456789';
const drops = [];
const fontSize = 14;
const columns = canvas.width/fontSize;

for(let x = 0; x < columns; x++) {
    drops[x] = 1;
}

function draw() {
    ctx.fillStyle = 'rgba(0, 0, 0, 0.05)';
    ctx.fillRect(0, 0, canvas.width, canvas.height);
    
    ctx.fillStyle = '#0f0';
    ctx.font = fontSize + 'px monospace';

    for(let i = 0; i < drops.length; i++) {
        const text = chars[Math.floor(Math.random() * chars.length)];
        ctx.fillText(text, i*fontSize, drops[i]*fontSize);
        
        if(drops[i]*fontSize > canvas.height && Math.random() > 0.975)
            drops[i] = 0;
        
        drops[i]++;
    }
}

setInterval(draw, 35);

// Make the frog hop
const frog = document.getElementById('frog');
let hopDirection = 1;

function hopFrog() {
    frog.style.transform = `translateY(${-10 * hopDirection}px)`;
    setTimeout(() => {
        frog.style.transform = 'translateY(0)';
    }, 500);
}

setInterval(hopFrog, 2000);

// Terminal typing effect
let currentText = '';
const texts = [
  'Loading FWOG AI...',
  'Initializing cute protocols...',
  'Ribbit ribbit...',
  'AI systems: OPERATIONAL'
];

let textIndex = 0;
let charIndex = 0;

function typeEffect() {
  if (charIndex < texts[textIndex].length) {
    currentText += texts[textIndex].charAt(charIndex);
    charIndex++;
    setTimeout(typeEffect, 100);
  } else {
    setTimeout(() => {
      currentText = '';
      charIndex = 0;
      textIndex = (textIndex + 1) % texts.length;
      typeEffect();
    }, 2000);
  }
}

typeEffect();
</script>
